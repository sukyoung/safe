  var astroArr = 0;
  var astroYunshi = "80";
  var astroUrl = "http://data.astro.qq.com/dayastro/70/70503/index.shtml";
  var astroContent = "";
  QQ.LoadScript("http://app.data.qq.com/?umod=astro&act=astro&t=18&jsonp=1&func=qqastro", 

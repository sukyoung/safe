                if(tubd_qhome_yw.hot[i].ext.style.color == "2")
                {
                  tubdYwArr.push('qm_c_2 ');
                }
                else
                  if(tubd_qhome_yw.hot[i].ext.style.color == "3")
                  {
                    tubdYwArr.push('qm_c_3 ');
                  }
              if(tubd_qhome_yw.hot[i].ext.style.icon == "1")
              {

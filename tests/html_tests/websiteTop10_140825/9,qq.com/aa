                  if(tubd_qhome_yw.hot[i].items[n].ext.style.icon == "2")
                  {
                    tubdYwArr.push('qm_i_2');
                  }
                  else
                    if(tubd_qhome_yw.hot[i].items[n].ext.style.icon == "3")
                    {
                      tubdYwArr.push('qm_i_3');
                    }
                    else
                      if(tubd_qhome_yw.hot[i].items[n].ext.style.icon == "4")
